{
    "name": "tumblr-tile",
    "version": "0.1",
    "icons": {"128": "images/headshot.png"},
    "description": "tumblr-tile",
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "permissions": [
        "https://api.tumblr.com/"
    ],
    "options_page": "options.html",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://api.tumblr.com; object-src 'self'"
}
